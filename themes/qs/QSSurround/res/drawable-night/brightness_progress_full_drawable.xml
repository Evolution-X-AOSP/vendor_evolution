<?xml version="1.0" encoding="utf-8" ?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@*android:id/background"
        android:gravity="fill_horizontal|center">
        <shape android:shape="rectangle">
            <corners android:radius="@*android:dimen/autofill_dialog_corner_radius" />
            <size android:height="48.0dip" />
            <solid android:color="@*android:color/system_neutral2_800" />
        </shape>
    </item>
    <item
        android:id="@*android:id/progress"
        android:gravity="fill_horizontal|center">
        <clip
            android:clipOrientation="horizontal"
            android:gravity="left">
            <selector>
                <item>
                    <shape android:shape="rectangle">
                        <corners android:radius="@*android:dimen/autofill_dialog_corner_radius" />
                        <size android:height="48.0dip" />
                        <gradient
                            android:angle="0.0"
                            android:endColor="@*android:color/system_accent2_400"
                            android:startColor="@*android:color/system_accent1_400" />
                    </shape>
                </item>
            </selector>
        </clip>
    </item>
</layer-list>

