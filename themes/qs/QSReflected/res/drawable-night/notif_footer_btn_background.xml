<?xml version="1.0" encoding="utf-8" ?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="2dp"
        android:top="4dp"
        android:start="2dp">
        <shape android:shape="rectangle">
            <gradient
                android:startColor="@*android:color/system_accent1_400"
                android:endColor="@*android:color/system_accent1_400"
                android:angle="315.0" />
            <corners android:radius="@*android:dimen/autofill_dialog_corner_radius" />
        </shape>
    </item>
    <item
        android:bottom="4dp"
        android:top="2dp"
        android:end="2dp">
        <shape android:shape="rectangle">
            <solid android:color="@*android:color/system_neutral2_800" />
            <corners android:radius="@*android:dimen/autofill_dialog_corner_radius" />
            <padding
                android:left="20dp"
                android:right="20dp" />
        </shape>
    </item>
</layer-list>

